# Copy this file to .streamlit/secrets.toml and fill in your credentials.
# Values can also be provided through Streamlit Cloud secrets or environment variables.

# Optional SQLAlchemy connection string. When omitted, the app falls back to SQLite (db.sqlite3).
# DATABASE_URL = "postgresql+psycopg2://user:password@host:5432/database"

# Ozon Seller API credentials for the Ozon Products page.
# OZON_CLIENT_ID = "your_client_id"
# OZON_API_KEY = "your_api_key"

# Wildberries API token for synchronization on the WB products page.
# WB_API_TOKEN = "your_wb_api_token"
# Optional overrides for Wildberries API base URL and endpoints.
# WB_API_BASE = "https://suppliers-api.wildberries.ru"
# WB_API_ENDPOINT_CARDS_V2 = "/content/v2/get/cards/list"
# WB_API_ENDPOINT_CARDS_CURSOR_V1 = "/content/v1/cards/cursor/list"
